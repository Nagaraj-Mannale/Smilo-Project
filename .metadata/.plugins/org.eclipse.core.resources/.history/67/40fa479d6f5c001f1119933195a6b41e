package practice;

public class SwitchCase {
public static void main(String[] args) {
	String month="Mar";
	switch(month)
	{
	case "Jan": System.out.println(1+" Month");break;
	case "Feb": System.out.println(2+" Month");break;
	case "Mar": System.out.println(3+" Month");break;
	case "Apr": System.out.println(4+" Month");break;
	case "May": System.out.println(5+" Month");break;
	case "Jun": System.out.println(6+" Month");break;
	case "Jul": System.out.println(7+" Month");break;
	case "Aug": System.out.println(8+" Month");break;
	case "Sep": System.out.println(9+" Month");break;
	case "Oct": System.out.println(10+" Month");break;
	case "Nov": System.out.println(11+" Month");break;
	default:System.out.println(12+" Month");
	}
}
}
